  object QGUN: TOraQuery
    SQL.Strings = (
      'SELECT TO_CHAR(T.GUN, '#39'DAY'#39') GUN'
      '  FROM TABLE(tarih_getir(to_number(:yil) )) T'
      ' WHERE TO_NUMBER((TO_CHAR(T.GUN, '#39'MM'#39'))) =to_number(:ay)')
    Left = 448
    Top = 19
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'yil'
        Value = nil
      end
